{
    "plugins": [
        [ "transform-object-rest-spread", {
            "useBuiltIns": true
        } ]
    ],
    "presets": [
        [ "env", {
            "targets": {
                "node": 6,
                "browsers": [
                    "last 2 Chrome versions",
                    "last 2 Firefox versions",
                    "last 2 Safari versions",
                    "last 2 Edge versions",
                    "ie >= 11"
                ]
            },
            "modules": false
        } ]
    ],
    "env": {
        "test": {
            "plugins": [ "istanbul" ],
            "presets": [
                [ "env", {
                    "targets": {
                        "node": 6
                    },
                    "modules": "commonjs"
                } ]
            ]
        }
    }
}
